{
    "container": "docker.io/library/node:22.20-alpine",
    "install": "/code/scripts/installPrerequisites.sh && /code/scripts/installPersisto.sh",
    "enable": [
        "postgres",
        "keycloak",
        "llamaindex"
    ],
    "env": [
        "OPENAI_API_KEY",
        "ANTHROPIC_API_KEY",
        "GEMINI_API_KEY",
        "MISTRAL_API_KEY",
        "DEEPSEEK_API_KEY",
        "OPENROUTER_API_KEY",
        "HUGGINGFACE_API_KEY",
        "LLMAgentClient_DEBUG",
        "LLMAgentClient_VERBOSE_DELAY",
        "PERSISTO_URL",
        "PERSISTO_HOST",
        "PERSISTO_PORT"
    ],
    "cli": "/code/scripts/startFlow.sh",
    "agent": "node ./Persisto/src/persistoServer.cjs",
    "ploinky": "sso enable"
}
